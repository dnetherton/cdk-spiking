{
  "name": "spike2",
  "version": "0.1.0",
  "bin": {},
  "watch": {
    "build": {
      "patterns": [
        "handlers"
      ],
      "extensions": "js",
      "quiet": true,
      "legacyWatch": false,
      "delay": 1000,
      "runOnChangeOnly": false,
      "inherit": true
    }
  },
  "scripts": {
    "watch": "./scripts/build.sh --watch",
    "build": "./scripts/build.sh",
    "package": "./scripts/package.sh",
    "deploy": "./scripts/deploy.sh",
    "deploy:code:only": "./scripts/deploy-code-only.sh",
    "start": "./scripts/start.sh",
    "start:local:db": "./scripts/start-local-db.sh",
    "postinstall": "./scripts/postinstall.sh || true",
    "test": "tsc",
    "test:watch": "tsc -w",
    "cdk": "cdk",
    "logs": "./scripts/logs.sh",
    "lint": "eslint --fix handlers stacks index.js",
    "lint:nofix": "eslint ."
  },
  "devDependencies": {
    "@aws-cdk/aws-ecs": "^1.4.0",
    "@aws-cdk/aws-ecs-patterns": "^1.4.0",
    "@aws-cdk/aws-iam": "^1.4.0",
    "@aws-cdk/aws-lambda": "^1.4.0",
    "@aws-cdk/aws-lambda-event-sources": "^1.4.0",
    "@aws-cdk/aws-rds": "^1.4.0",
    "@aws-cdk/aws-sam": "^1.4.0",
    "@aws-cdk/aws-serverless": "^0.28.0",
    "@aws-cdk/aws-sns": "^1.4.0",
    "@aws-cdk/aws-sns-subscriptions": "^1.4.0",
    "@aws-cdk/aws-sqs": "^1.4.0",
    "@aws-cdk/cdk": "^0.36.1",
    "@aws-cdk/core": "^1.4.0",
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.0",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-proposal-do-expressions": "^7.5.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.4.4",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-proposal-partial-application": "^7.4.4",
    "@babel/plugin-proposal-pipeline-operator": "^7.5.0",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "@babel/plugin-transform-strict-mode": "^7.2.0",
    "@babel/preset-env": "^7.5.4",
    "@babel/register": "^7.5.5",
    "aws-cdk": "^1.4.0",
    "babel-eslint": "^11.0.0-beta.0",
    "babel-loader": "^8.0.6",
    "cdk": "^1.4.0",
    "mysql2": "^1.6.5",
    "terser-webpack-plugin": "^1.3.0",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.6",
    "eslint": "^5.16.0",
    "eslint-config-google": "^0.12.0",
    "eslint-config-stickler": "^1.0.5",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-no-only-tests": "^2.3.1",
    "eslint-plugin-node": "^8.0.1"
  },
  "dependencies": {
    "express": "^4.17.1",
    "knex": "^2.3.0"
  }
}
